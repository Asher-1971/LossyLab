{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/LossyLab/bin/Debug/net8.0/LossyLab.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Production",
                "DOTNET_CLI_TELEMETRY_OPTOUT": "1"
            },
            "logging": {
                "moduleLoad": false
            }
        }
    ]
}